{
  "version": 2,
  "projects": {
    "test": {
      "root": "apps/test",
      "sourceRoot": "apps/test/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/test",
            "index": "apps/test/src/index.html",
            "main": "apps/test/src/main.tsx",
            "polyfills": "apps/test/src/polyfills.ts",
            "tsConfig": "apps/test/tsconfig.app.json",
            "assets": [
              "apps/test/src/favicon.ico",
              "apps/test/src/assets"
            ],
            "styles": [
              "apps/test/src/styles.css"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/test/src/environments/environment.ts",
                  "with": "apps/test/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "test:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "test:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/test/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/test"
          ],
          "options": {
            "jestConfig": "apps/test/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "test-e2e": {
      "root": "apps/test-e2e",
      "sourceRoot": "apps/test-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/test-e2e/cypress.json",
            "tsConfig": "apps/test-e2e/tsconfig.e2e.json",
            "devServerTarget": "test:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "test:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/test-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "mobile-app": {
      "root": "apps/mobile-app",
      "sourceRoot": "apps/mobile-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/mobile-app",
            "index": "apps/mobile-app/src/index.html",
            "main": "apps/mobile-app/src/main.tsx",
            "polyfills": "apps/mobile-app/src/polyfills.ts",
            "tsConfig": "apps/mobile-app/tsconfig.app.json",
            "assets": [
              "apps/mobile-app/src/assets",
              "apps/mobile-app/src/manifest.json"
            ],
            "styles": [
              "apps/mobile-app/src/styles.css"
            ],
            "scripts": [],
            "webpackConfig": "@nxtend/ionic-react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/mobile-app/src/environments/environment.ts",
                  "with": "apps/mobile-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "mobile-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "mobile-app:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/mobile-app/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/mobile-app"
          ],
          "options": {
            "jestConfig": "apps/mobile-app/jest.config.js",
            "passWithNoTests": true
          }
        },
        "add": {
          "executor": "@nxtend/capacitor:command",
          "options": {
            "command": "add",
            "platform": ""
          },
          "configurations": {
            "ios": {
              "platform": "ios"
            },
            "android": {
              "platform": "android"
            }
          }
        },
        "copy": {
          "executor": "@nxtend/capacitor:command",
          "options": {
            "command": "copy",
            "platform": ""
          },
          "configurations": {
            "platform": "android",
            "ios": {
              "platform": "ios"
            },
            "android": {
              "platform": "android"
            }
          }
        },
        "open": {
          "executor": "@nxtend/capacitor:command",
          "options": {
            "command": "open",
            "platform": ""
          },
          "configurations": {
            "platform": "android",
            "ios": {
              "platform": "ios"
            },
            "android": {
              "platform": "android"
            }
          }
        },
        "sync": {
          "executor": "@nxtend/capacitor:command",
          "options": {
            "command": "sync",
            "platform": ""
          },
          "configurations": {
            "platform": "android",
            "ios": {
              "platform": "ios"
            },
            "android": {
              "platform": "android"
            }
          }
        },
        "update": {
          "executor": "@nxtend/capacitor:command",
          "options": {
            "command": "update",
            "platform": ""
          },
          "configurations": {
            "platform": "android",
            "ios": {
              "platform": "ios"
            },
            "android": {
              "platform": "android"
            }
          }
        }
      }
    },
    "mobile-app-e2e": {
      "root": "apps/mobile-app-e2e",
      "sourceRoot": "apps/mobile-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/mobile-app-e2e/cypress.json",
            "tsConfig": "apps/mobile-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "mobile-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mobile-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/mobile-app-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nxtend/ionic-react": {
      "application": {
        "style": "css",
        "linter": "eslint"
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "test"
}